.tags{
    display: flex;
    margin-bottom: 35px;
    &__item{
        background-color: #F3F6F9;
        border-radius: 50px;
        padding-left: 22px;
        padding-right: 22px;
        height: 48px;
        line-height: 48px;
        margin-right: 8px;
        display: inline-block;
        &:last-child{
            margin-right: 0;
        }
    }
}